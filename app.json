{
  "expo": {
    "name": "CalorIA",
    "slug": "CalorIA",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "scheme": "caloria",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.CalorIA",
      "deploymentTarget": "26.0",
      "infoPlist": {
        "NSCameraUsageDescription": "CalorIA necesita acceso a la cámara para tomar fotos de alimentos y calcular calorías automáticamente.",
        "NSPhotoLibraryUsageDescription": "CalorIA necesita acceso a la galería para seleccionar fotos de alimentos existentes."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.anonymous.CalorIA",
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-font",
      "expo-camera",
      "expo-image-picker"
    ],
    "extra": {
      "eas": {
        "projectId": "aa27fcc8-987b-44d0-bfb8-daa66c9b454b"
      }
    }
  }
}
